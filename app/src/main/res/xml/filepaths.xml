<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!-- 表示Environment.getExternalStorageDirectory()目录或者其子目录。 /storage/emulated/0/Download/${applicationId}/.beta/apk-->
    <external-path name="beta_external_path" path="Download/"/>
    <external-path name="beta_external_path" path=""/>
    <!--/storage/emulated/0/Android/data/${applicationId}/files/apk/-->
    <external-path name="beta_external_files_path" path="Android/data/"/>
    <external-path name="beta_external_files_path" path=""/>
    <!--表示Context.getFilesDir()目录或者其子目录。  /data/data/${applicationId}/files/ -->
    <files-path name="name1" path="" />
    <!--表示Context.getCacheDir()目录或者其子目录。   /data/data/${applicationId}/cache/-->
    <cache-path name="name2" path="" />
    <!--表示Context.getExternalFilesDir(null)目录或者其子目录   示例 : /storage/emulated/0/Android/data/${applicationId}/files/test4-->
    <external-files-path name="name4" path="" />
    <!--表示Context.getExternalCacheDir()目录或者其子目录。   /storage/emulated/0/Android/data/com.chen.gradle/cache/test5-->
    <external-cache-path name="name5" path="" />
</paths>